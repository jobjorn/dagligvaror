{
  "name": "dagligvaror",
  "version": "1.0.0",
  "description": "A grocery management application",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "vercel-build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "test": "jest",
    "test-watch": "jest --watch",
    "prisma:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:push:local": "dotenv -e .env.local -- npx prisma db push",
    "prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "lint": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.6.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@eslint/config-array": "^0.19.2",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/material-nextjs": "^6.4.3",
    "@mui/system": "^6.4.8",
    "@mui/x-date-pickers": "^8.15.0",
    "@prisma/client": "^6.5.0",
    "date-fns": "^4.1.0",
    "iconv-lite": "^0.6.3",
    "next": "^14.2.3",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "recharts": "^2.12.7",
    "sass": "^1.77.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.11.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "prisma": "^6.5.0",
    "typescript": "^5.4.5"
  }
}
